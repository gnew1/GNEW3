{ 
  "compilerOptions": { 
    "target": "ES2022", 
    "lib": ["ES2023"], 
    "module": "ESNext", 
    "moduleResolution": "Bundler", 
    "types": ["node"], 
    "declaration": true, 
    "stripInternal": true, 
    "resolveJsonModule": true, 
    "skipLibCheck": true, 
    "allowJs": false, 
    "checkJs": false, 
    "forceConsistentCasingInFileNames": true, 
    "noErrorTruncation": true 
  } 
} 
 
